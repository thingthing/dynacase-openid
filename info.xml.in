<?xml version="1.0"?>
<module name="@PACKAGE@" disabled="no" version="@VERSION@" release="@RELEASE@">

  <description lang="fr">Support authentification OPENID pour dynacase-platform</description>
  <description lang="en">OPENID authentification support for  dynacase-platform</description>

  <requires>
    <module name="dynacase-networkuser" />
  </requires>

  <changelog>
    <version number="1.0" date="2010-07-28">
    	<change title="Initial release" />
    </version>
  </changelog>
  
   <pre-install>
    <check type="phpfunction" function="curl_exec">
      <help>Please install php5-curl before installing openid module</help>
    </check>
  </pre-install>
  
 <pre-update>
    <check type="phpfunction" function="curl_exec">
      <help>Please install php5-curl before updating openid module</help>
    </check>
  </pre-update>
</module>
